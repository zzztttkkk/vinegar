{
	"$schema": "https://biomejs.dev/schemas/1.9.2/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": []
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"lineWidth": 120
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "off"
			},
			"complexity": {
				"noBannedTypes": "off",
				"noStaticOnlyClass": "off",
				"noThisInStatic": "off"
			},
			"style": {
				"noUnusedTemplateLiteral": "off",
				"noParameterAssign": "off",
				"noInferrableTypes": "off",
				"noNonNullAssertion": "off"
			}
		}
	},
	"javascript": {
		"formatter": {}
	}
}
